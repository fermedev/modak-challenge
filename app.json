{
  "expo": {
    "name": "modak-challenge",
    "slug": "modak-challenge",
    "version": "1.0.0",

    "icon": "./assets/images/icon.png",
    "scheme": "modak-products",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ferjmedinadev.modakchallenge",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs calendar access to add purchase reminders."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ferjmedinadev.modakchallenge"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
